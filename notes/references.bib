  author = {Xue, X. X. and Rix, H. W. and Zhao, G. and Re Fiorentin, P. and Naab, T. and Steinmetz, M. and {van den Bosch}, F. C. and Beers, T. C. and Lee, Y. S. and Bell, E. F. and Rockosi, C. and Yanny, B. and Newberg, H. and Wilhelm, R. and Kang, X. and Smith, M. C. and Schneider, D. P.},
