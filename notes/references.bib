  issn = {0004-637X},
  doi = {10.3847/1538-4357/ab910c},
  abstract = {A star in the Milky Way's disk can now be at a Galactocentric radius quite distant from its birth radius for two reasons: either its orbit has become eccentric through radial heating, which increases its radial action \$J\_R\$ (`blurring'); or merely its angular momentum \$L\_z\$ has changed and thereby its guiding radius (`churning'). We know that radial orbit migration is strong in the Galactic low-\$\textbackslash alpha\$ disk and set out to quantify the relative importance of these two effects, by devising and applying a parameterized model for the distribution \$p(L\_z, J\_R, \textbackslash tau, \textbackslash mathrm[Fe/H])\$ in the stellar disk. This model describes the orbit evolution for stars of age \$\textbackslash tau\$ and metallicity [Fe/H], presuming coeval stars were initially born on (near-)circular orbits, and with a unique [Fe/H] at a given birth angular momentum and age. We fit this model to APOGEE red clump stars, accounting for the complex selection function of the survey. The best fit model implies changes of angular momentum of \$\textbackslash sqrt\{\textbackslash langle \textbackslash Delta L\_z \textbackslash rangle\^2\} \textbackslash approx 619\textbackslash, \textbackslash mathrm\{kpc\textasciitilde km/s\textasciitilde\}(\textbackslash tau/\textbackslash mathrm\{6\textasciitilde Gyr\})\^\{0.5\}\$, and changes of radial action as \$\textbackslash sqrt\{\textbackslash langle \textbackslash Delta J\_R \textbackslash rangle\^2\} \textbackslash approx 63\textbackslash, \textbackslash mathrm\{kpc\textasciitilde km/s\textasciitilde\} (\textbackslash tau/\textbackslash mathrm\{6\textasciitilde Gyr\})\^\{0.6\}\$ at 8 kpc. This suggests that the secular orbit evolution of the disk is dominated by diffusion in angular momentum, with radial heating being an order of magnitude lower.},
  file = {/Users/js5013/Zotero/storage/G95ZVEA3/Frankel_2020_ApJ_896_15.pdf}
}

@article{Fritz2018,
