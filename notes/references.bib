@misc{Midgley2022,
  title = {Flow {{Annealed Importance Sampling Bootstrap}}},
  author = {Midgley, Laurence Illing and Stimper, Vincent and Simm, Gregor N. C. and Sch{\"o}lkopf, Bernhard and {Hern{\'a}ndez-Lobato}, Jos{\'e} Miguel},
