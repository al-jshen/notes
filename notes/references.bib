  author = {Marinacci, Federico and Vogelsberger, Mark and Pakmor, R{\"u}diger and Torrey, Paul and Springel, Volker and Hernquist, Lars and Nelson, Dylan and Weinberger, Rainer and Pillepich, Annalisa and Naiman, Jill and Genel, Shy},
  year = {2018},
  month = aug,
  journal = {Monthly Notices of the Royal Astronomical Society},
