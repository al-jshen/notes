  author = {Fukushima, Tetsuya and Chiba, Masashi and Tanaka, Mikito and Hayashi, Kohei and Homma, Daisuke and Okamoto, Sakurako and Komiyama, Yutaka and Tanaka, Masayuki and Arimoto, Nobuo and Matsuno, Tadafumi},
  year = {2019},
  journal = {Publications of the Astronomical Society of Japan},
  volume = {71},
