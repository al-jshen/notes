  author = {Rudnick, Gregory and Hodge, Jacqueline and Walter, Fabian and Momcheva, Ivelina and Tran, Kim-Vy and Papovich, Casey and {da Cunha}, Elisabete and Decarli, Roberto and Saintonge, Amelie and Willmer, Christopher and Lotz, Jennifer and Lentati, Lindley},
  year = {2017},
