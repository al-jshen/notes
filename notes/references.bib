@article{Eadie2015,
  title = {Estimating the {{Galactic Mass Profile}} in the {{Presence}} of {{Incomplete Data}}},
  author = {Eadie, Gwendolyn M. and Harris, William E. and Widrow, Lawrence M.},
  year = {2015},
  month = mar,
  journal = {The Astrophysical Journal},
