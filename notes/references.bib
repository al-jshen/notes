@article{Elbaz2017a,
  title = {Starbursts in and out of the Star-Formation Main Sequence},
  author = {Elbaz, David and Leiton, Roger and Nagar, Neil and Okumura, Koryo and Franco, Maximilien and Schreiber, Corentin and Pannella, Maurilio and Wang, Tao and Dickinson, Mark and {Diaz-Santos}, Tanio and Ciesla, Laure and Daddi, Emanuele and Bournaud, Frederic and Magdis, Georgios and Zhou, Luwenjia and Rujopakarn, Wiphu},
  year = {2017},
  month = nov,
  journal = {Astronomy and Astrophysics},
  volume = {616},
