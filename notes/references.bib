  file = {/Users/js5013/Zotero/storage/BS8697IZ/Frankel_2019_ApJ_884_99.pdf}
}

@article{Frankel2020,
  title = {Keeping {{It Cool}}: {{Much Orbit Migration}}, yet {{Little Heating}}, in the {{Galactic Disk}}},
  author = {Frankel, Neige and Sanders, Jason and Ting (丁源森), Yuan-Sen and Rix, Hans-Walter},
  year = {2020},
